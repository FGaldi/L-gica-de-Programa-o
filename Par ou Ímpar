#include<stdio.h>

int main (){
    int i;
    int N;
    int X;

    scanf("%i", &N);

    for(i = 1; i <= N; i++){
        scanf("%i", &X);

        if(X == 0){
            printf("NULL\n");
        }else{
            if(X % 2 == 0) printf("EVEN ");
            else printf("ODD ");


            if(X > 0) printf("POSITIVE\n");
            else printf("NEGATIVE\n");
        }
    }

    return 0;

}
