#include <stdio.h>

int main(){
    double N, media;
    double soma=0;
    int i;
    int p=0;
    
    for( i = 1 ; i <= 6 ; i++){
        
        scanf("%lf", &N);
        if( N > 0.0 ){
            p++;
            soma = soma + N;
        }
    }
    
    media = soma/p;
    printf("%i valores positivos\n", p);
    
    printf("%.1lf\n", media);
    
    return 0;
    
}
